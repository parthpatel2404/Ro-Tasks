@model List<CIPlatform.Entities.Models.SearchResult>

@{
    ViewData["Title"] = "Home Page";
}

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Custom Google Page</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />
    <link rel="stylesheet" href="~/css/style.css">

</head>

<body>
    <div class="container">
        <div class="row mt-5">

            @using (Html.BeginForm())
            {
                <div>
                    <input type="text" name="search" placeholder="Enter your search query" />
                    <button type="submit">Search</button>
                </div>
            }

            @if (Model != null && Model.Count > 0)
            {
                <ul>
                    @foreach (var result in Model)
                    {
                        <li>
                            <h3>@result.Title</h3>
                            <p>@result.Snippet</p>
                            <a href="@result.Url" target="_blank">@result.Url</a>
                        </li>
                    }
                </ul>
            }

            @*            <div>
                <form asp-action="CustomGoogle" class="form-floating w-75 d-flex">
                <input type="search" name="search" class="form-control"
                placeholder="" />
                <label for="floatingSearch" class="d-flex align-items-center ms-2">Search</label>
                <button type="submit" class="btn btn-outline-dark ms-2">Submit</button>
                </form>
                </div>
            *@
        </div>

    </div>
</body>

</html>
